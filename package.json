{
  "name": "auto-collapse-explorer",
  "displayName": "auto-collapse-explorer",
  "description": "Auto-collapse explorer directories that are not currently in view.",
  "version": "1.0.3",
  "engines": {
    "vscode": "^1.68.1"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/extension.js",
  "contributes": {},
  "repository": {
    "type": "git",
    "url": "https://github.com/LukeGeneva/vscode-auto-collapse-explorer.git"
  },
  "scripts": {
    "deploy": "vsce publish",
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.68.1",
    "@typescript-eslint/eslint-plugin": "^5.30.0",
    "@typescript-eslint/parser": "^5.30.0",
    "@vscode/test-electron": "^2.1.5",
    "eslint": "^8.18.0",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "typescript": "^4.7.4"
  }
}
